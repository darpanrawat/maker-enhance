{
  "name": "maker-enhance",
  "version": "0.0.1",
  "description": "React component for Maker Enhance",
  "keywords": ["react", "react-component", "maker", "embed"],
  "main": "lib/index.js",
  "author": "Pavel Linkesch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makerinc/maker-enhance.git"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0",
    "react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1"
  },
  "scripts": {
    "prettier:fix": "prettier --write \"{src,spec}/**/*.js\"",
    "eslint": "eslint \"{src,spec}/**/*.js\"",
